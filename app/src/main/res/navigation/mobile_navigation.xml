<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_live_tv">

    <fragment
        android:id="@+id/navigation_live_tv"
        android:name="com.abdulwahedniaz.baryatv.ui.livetv.LiveTvFragment"
        android:label="@string/title_live_tv"
        tools:layout="@layout/fragment_live_tv" />

    <fragment
        android:id="@+id/navigation_youtube"
        android:name="com.abdulwahedniaz.baryatv.ui.youtube.YouTubeFragment"
        android:label="@string/title_youtube"
        tools:layout="@layout/fragment_youtube" />

    <fragment
        android:id="@+id/navigation_facebook"
        android:name="com.abdulwahedniaz.baryatv.ui.facebook.FacebookFragment"
        android:label="@string/title_facebook"
        tools:layout="@layout/fragment_facebook" />

    <fragment
        android:id="@+id/navigation_about_us"
        android:name="com.abdulwahedniaz.baryatv.ui.aboutus.AboutUsFragment"
        android:label="@string/title_about_us"
        tools:layout="@layout/fragment_about_us" />
</navigation>